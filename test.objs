enum ElementTag {
    Text | Block
}

typedef Element {
    field type -> ElementTag
    field text -> String

    ctor(type -> ElementTag, text -> String) {
        this.type = tag
        this.text = text
    }

    serialize() {
        let tag = type == ElementTag.Text ? 'p' : 'div'
        return '<' + tag + '>' + this.text + '</' + tag + '>'
    }
}

print(new Element(ElementTag.Text, 'Hello!'))